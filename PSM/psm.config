<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <configSections>
    <section name="master" type="PSMonitor.Setup, PSM" requirePermission="false" />
    <section name="stores" type="PSMonitor.Stores.Setup, PSM" requirePermission="false" />
  </configSections>

  <master scriptDirectory="C:\temp" defaultTimeout="15000" defaultInterval="60000" defaultNamespace="root" timerFrequency="100"/>
  
  <stores type="PSMonitor.Stores.Dummy">
    <settings>
      <add for="PSMonitor.Stores.HTTP" name="url" value="http://localhost:54926/" />
      <add for="PSMonitor.Stores.HTTP" name="connectionTimeout" value="5000" />
      <add for="PSMonitor.Stores.DB" name="connectionString" value="Data Source=(local)\ADVANTAGE2005;Initial Catalog=advantage;Integrated Security=True;Connect Timeout=30;Encrypt=False;TrustServerCertificate=False;ApplicationIntent=ReadWrite;MultiSubnetFailover=False" />
      <add for="PSMonitor.Stores.DB" name="maxAge" value="0" />
      <add for="PSMonitor.Stores.DB" name="pollingInterval" value="5000" />
    </settings>
  </stores>

</configuration>